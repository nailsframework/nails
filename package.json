{
  "name": "@nailsframework/nails",
  "version": "1.1.7",
  "description": "The Typescript Library",
  "main": "lib/nails.js",
  "types": "lib/nails.d.ts",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "tsc": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nailsframework/nails.git"
  },
  "keywords": [
    "Nails",
    "Framework",
    "nailsframework",
    "nails",
    "nailsjs",
    "nailsts",
    "frontend",
    "javascript",
    "typescript",
    "library",
    "framework"
  ],
  "author": "Dominic JÃ¤rmann",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nailsframework/nails/issues"
  },
  "homepage": "https://github.com/nailsframework/nails#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/preset-typescript": "^7.7.2",
    "@types/jest": "^24.0.22",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "ts-polyfill": "^3.5.3"
  }
}
